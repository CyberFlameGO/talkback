{
  "name": "talkback",
  "version": "1.12.0",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "A node.js HTTP proxy that records and playbacks requests",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "buffer-shims": "^1.0.0",
    "content-type": "^1.0.4",
    "json5": "^2.1.1",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.1.2",
    "cross-env": "^7.0.0",
    "del": "^5.1.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.0.3",
    "testdouble": "^3.12.5"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ijpiantanida/talkback"
  },
  "keywords": [
    "http-proxy",
    "record",
    "vcr",
    "playback"
  ],
  "author": "Ignacio Piantanida",
  "scripts": {
    "build": "node tools/build.js",
    "ci": "yarn test && yarn build && USE_DIST=1 yarn test",
    "start": "node ./example/start.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha -r mocha-setup.js --recursive test"
  }
}
